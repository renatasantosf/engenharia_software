<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="principalEmprestimo" prefHeight="515.0" prefWidth="911.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.senacrs.biblioteca.controller.EmprestimoController">
   <children>
      <ToolBar fx:id="toolPrincipal" prefHeight="41.0" prefWidth="911.0">
         <items>
            <Button fx:id="btIncluir" mnemonicParsing="false" onAction="#tratarBotaoCadastrar" text="Incluir" />
            <Button fx:id="btRemover" mnemonicParsing="false" onAction="#tratarBotaoRemover" text="Remover" />
            <TextField fx:id="tfBuscaCodigo" prefHeight="25.0" prefWidth="223.0" />
            <Button fx:id="btBuscaEmprestimo" mnemonicParsing="false" onAction="#buscarEmprestimo" text="Buscar Empréstimo" />
         </items>
      </ToolBar>
      <TableView fx:id="tableViewEmprestimo" layoutY="40.0" prefHeight="480.0" prefWidth="911.0">
         <columns>
            <TableColumn fx:id="tableColumnCodigo" prefWidth="97.0" text="Código" />
            <TableColumn fx:id="tableColumnMatricula" prefWidth="209.0" text="Matrícula" />
            <TableColumn fx:id="tableColumnLivro" prefWidth="239.0" text="Livro" />
            <TableColumn fx:id="tableColumnDtEmprestimo" prefWidth="173.0" text="Data Empréstimo" />
            <TableColumn fx:id="tableColumnDtEntrega" prefWidth="190.0" text="Data Entrega" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
